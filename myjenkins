pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            
    stage('Build') {
            steps {
                echo 'Hello World is Building'
            }
        }
         stage('Testing') {
            steps {
                echo 'Hello World is Testing'
            }
        }
        
    }
}
